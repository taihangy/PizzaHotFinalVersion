<!-- Banner Start -->

<div class="banner padd">
	<div class="container">
		<!-- Image -->
		<%= image_tag("crown-white.png", class: "img-responsive")%>
		<!-- Heading -->
		<h2 class="white">About Us</h2>
		<ol class="breadcrumb">
			<li><a href="index.html">Home</a></li>
			<li class="active">About Us</li>
